{
  "name": "my-app",
  "type": "module",
  "scripts": {
    "dev": "wrangler dev",
    "db:reset:local": "wrangler d1 execute my-db --local --command=\"DROP TABLE IF EXISTS episodes;DROP TABLE IF EXISTS d1_migrations;\" && wrangler d1 migrations apply my-db --local",
    "deploy": "wrangler deploy --minify",
    "cf-typegen": "wrangler types --env-interface CloudflareBindings"
  },
  "dependencies": {
    "hono": "^4.9.9"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20251001.0",
    "@eslint/js": "^9.38.0",
    "@hono/eslint-config": "^2.0.3",
    "eslint": "^9.38.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.2",
    "wrangler": "^4.45.1"
  }
}
